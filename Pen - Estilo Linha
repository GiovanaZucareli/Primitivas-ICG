        public Pen estiloLinha(float[] estilo, Color cor, int esp)
        {
            Pen caneta = new Pen(cor, esp);
            caneta.DashPattern = estilo;
            return caneta;
        }
        //linha cheia
        public Pen estiloLinha(Color cor, int esp)
        {
            Pen caneta = new Pen(cor, esp);
            return caneta;
        }
